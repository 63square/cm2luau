        p -= 1;
        buffer.writeu8(buf, p, a % 100)
        a //= a
    end

    if a < 10 then
        buffer.writeu8(b, offset + i, a + 0x30)
        i += 1
    elseif a < 100 then
        buffer.writeu16(b, offset + i, d100[a + 1])
        i += 2
    end

    for v = p, 4 do
        buffer.writeu16(b, offset + i, d100[buffer.readu8(buf, v) + 1])
        i += 2;
    end

    return i
end

--- creation functions
local creation = {}
function creation.addBlockNN(self: Creation, block: Block): BlockHandle
    -- block id
    local o = self.block_offset
    local b = self.block_data

    if block < 10 then 
        buffer.writeu16(b, o, fdp[block])
        o += 1
    else
        buffer.writeu32(b, o, ddp[block])
        o += 2
    end

    -- 4 commas
    buffer.writeu32(b, o, 0x2C2C2C2C)
    o += 4

    -- comma and semicolon
    buffer.writeu16(b, o, 0x3B2C)
    o += 2

    -- increment block count and return handle
    self.block_count += 1
    self.block_offset = o
    return self.block_count
end
function creation.addBlockN(self: Creation, block: Block, x: number, y: number, z: number): BlockHandle
    -- block id
    local o = self.block_offset
    local b = self.block_data

    if block < 10 then 
        buffer.writeu16(b, o, fdp[block])
        o += 1
    else
        buffer.writeu32(b, o, ddp[block])
        o += 2
    end

    -- double comma, ignore state
    buffer.writeu16(b, o, 0x2C2C)
    o += 2

    o += itoaPos(b, o, x)
    o += itoaPos(b, o, y)
    o += itoaPos(b, o, z)

    -- comma and semicolon
    buffer.writeu16(b, o, 0x3B2C)
    o += 2

    -- increment block count and return handle
    self.block_count += 1
    self.block_offset = o
    return self.block_count
end
function creation.addBlock(self: Creation, block: Block, x: number, y: number, z: number, properties: { number }): BlockHandle
    -- block id
    local o = self.block_offset
    local b = self.block_data

    if block < 10 then 
        buffer.writeu16(b, o, fdp[block])
        o += 1
    else
        buffer.writeu32(b, o, ddp[block])
        o += 2
    end

    -- double comma, ignore state
    buffer.writeu16(b, o, 0x2C2C)
    o += 2

    o += itoaPos(b, o, x)
    o += itoaPos(b, o, y)
    o += itoaPos(b, o, z)

    -- comma
    buffer.writeu8(b, o, 0x2C)
    o += 1

    for _, property in ipairs(properties) do
        -- value
        o += itoa(b, o, property)

        -- plus
        buffer.writeu8(b, o, 0x2B)
        o += 1
    end

    -- semicolon
    buffer.writeu8(b, o - 1, 0x3B)
    o += 1

    -- increment block count and return handle
    self.block_count += 1
    self.block_offset = o
    return self.block_count
end

function creation.connect(self: Creation, source: BlockHandle, target: BlockHandle): nil
    -- source
    self.connection_offset += itoa(self.connection_data, self.connection_offset, source)

    -- comma
    buffer.writeu8(self.connection_data, self.connection_offset, 0x2C)
    self.connection_offset += 1

    -- target
    self.connection_offset += itoa(self.connection_data, self.connection_offset, target)

    -- semicolon
    buffer.writeu8(self.connection_data, self.connection_offset, 0x3B)
    self.connection_offset += 1
    return
end

